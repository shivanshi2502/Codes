//numbers are already reversed and return reversed ans only

ListNode* addTwoNumbers(ListNode* l1, ListNode* l2) {
        ListNode* temp1=l1;
        ListNode* temp2=l2;
        int carry=0;
        ListNode* ansKaHead=new ListNode(-1);
        ListNode* prev=ansKaHead;
        int sum,digit;
        while(temp1!=NULL && temp2!=NULL){
            sum=temp1->val+temp2->val+carry;
            digit=sum%10;
            carry=sum/10;
            ListNode* newNode=new ListNode(digit);
            prev->next=newNode;
            prev=prev->next;
            temp1=temp1->next;
            temp2=temp2->next;
        }
        while(temp1!=NULL){
            sum=temp1->val+carry;
            digit=sum%10;
            carry=sum/10;
            ListNode* newNode=new ListNode(digit);
            prev->next=newNode;
            prev=prev->next;
            temp1=temp1->next;
        }
        while(temp2!=NULL){
            sum=temp2->val+carry;
            digit=sum%10;
            carry=sum/10;
            ListNode* newNode=new ListNode(digit);
            prev->next=newNode;
            prev=prev->next;
            temp2=temp2->next;
        }
        if(carry!=0){
            ListNode* newNode=new ListNode(carry);
            prev->next=newNode;
            prev=prev->next;
            carry=0;
        }
        ListNode*temp=ansKaHead;
        ansKaHead=ansKaHead->next;
        temp->next=NULL;
        delete temp;
        return ansKaHead;
    }
