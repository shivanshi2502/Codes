#include<stack>
vector<int> nextSmallerElement(vector<int> &arr, int n)
{
    vector<int> ans(n,-1);
    stack<int>st;
    st.push(-1);
    for(int i=arr.size()-1;i>=0;i--){
        if(arr[i]>st.top()){
            ans[i]=st.top();
            st.push(arr[i]);
        }
        else{
            while(arr[i]<=st.top()){
                st.pop();
            }
            ans[i]=st.top();
            st.push(arr[i]);
        }
    }
    return ans;
}
