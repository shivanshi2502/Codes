#include<vector>
#include<iostream>
#include<bits/stdc++.h>
using namespace std;

void solve(int arr[], int i,int n,int j){
    if(j==n){
        return;
    }
    for(int k=i;k<=j;k++){
        cout<<arr[k]<<" ";
    }
    cout<<endl;
    return solve(arr,i,n,j+1);
}
int main()
{
    int arr[]={1,2,3,4,5};
    int n=5;
    for(int i=0;i<n;i++){
        solve(arr,i,n,0);
    }
    return 0;
}
