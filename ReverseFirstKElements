#include <iostream>
#include<queue>
#include<stack>
using namespace std;
void reverseKElems(queue<int>&q,int &k){
    if(k==0){
        return;
    }
    stack<int>s;
    while(k!=0){
        int temp=q.front();
        q.pop();
        k--;
        s.push(temp);
    }
    queue<int>q2;
    while(!q.empty()){
        int temp=q.front();
        q.pop();
        q2.push(temp);
    }
    while(!s.empty()){
        int temp=s.top();
        s.pop();
        q.push(temp);
    }
    while(!q2.empty()){
        int temp=q2.front();
        q2.pop();
        q.push(temp);
    }
}
int main() {
    queue<int>q;
    q.push(3);
    q.push(6);
    q.push(9);
    q.push(12);
    q.push(15);
    int k=3;
    reverseKElems(q,k);
    while(!q.empty()){
        int it=q.front();
        cout<<it<<" ";
        q.pop();
    }
}
