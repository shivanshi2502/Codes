#include <iostream>

using namespace std;

class A{
    public:
    int a;
    int b;
    int c;
    
    int sum(int a,int b){
        return a+b;
    }
    
    int sum(int a,int b,int c){
        return a+b+c;
    }
    
    int sum(int a,float b){
        return a+b;
    }
};

int main()
{
    A obj;
    cout<<obj.sum(10,7.6f);
}
