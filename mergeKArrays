vector<int> mergeKArrays(vector<vector<int>> arr, int k)
    {
        vector<int>ans;
        priority_queue<int,vector<int>,greater<int>>pq;
        for(int i=0;i<k;i++){
            for(int j=0;j<k;j++){
                pq.push(arr[i][j]);
            }
        }
        while(!pq.empty()){
            int temp=pq.top();
            ans.push_back(temp);
            pq.pop();
        }
        return ans;
    }
