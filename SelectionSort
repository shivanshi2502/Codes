//selection sort

#include <iostream>
#include<vector>
using namespace std;

int main()
{
    vector<int> v{5,4,3,2,3,7,9,1,4};
    int n=v.size();
    for(int i=0;i<n-1;i++){
        int minIndex=i;
        for(int j=i+1;j<n;j++){
            if(v[minIndex]>v[j]){
                minIndex=j;
            }
        }
        swap(v[i],v[minIndex]);
    }
    //printing
    for(auto i: v){
        cout<<i<<" ";
    }
    return 0;
}
