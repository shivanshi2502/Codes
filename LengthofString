//Length of string

#include <iostream>
#include<string>
using namespace std;

int main()
{
    char st[1000];
    cin>>st;
    int count=0;
    for(int i=0;i<1000;i++){
        if(st[i]=='\0'){
            break;
        }
        else{
            count++;
        }
    }
    cout<<"length is "<<count<<endl;
}
