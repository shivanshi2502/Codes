//first non-repeating char in a string else print #

#include <iostream>
#include<queue>
#include<array>
using namespace std;

int main() {
    string str="zarcaazrd";
    string ans="";
    queue<char>q;
    int freq[26]={0};
    for(int i=0;i<str.length();i++){
        char ch=str[i];
        freq[ch-'a']++;
        q.push(ch);
        while(!q.empty()){
            if(freq[q.front()-'a']>1){
                q.pop();
            }
            else if(freq[q.front()-'a']==1){
                ans.push_back(q.front());
                break;
            }
        }
        if(q.empty()){
            ans.push_back('#');
        }
    }
    cout<<ans<<endl;
}
