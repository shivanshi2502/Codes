int kthSmallest(TreeNode* root, int &k) {
        if(root==NULL){
            return -1;
        }
        int left=kthSmallest(root->left,k);
        if(left!=-1){
            return left;
        }
        k--;
        if(k==0){
            k=-1;
            return root->val;
        }
        int right=kthSmallest(root->right,k);
        return right;
    }
