#include <iostream>
#include<stack>
#include<string>
using namespace std;

int main() {
    string st="shivanshi";
    stack<char> s;
    for(int i=0;i<st.length();i++){
        s.push(st[i]);
    }
    while(!s.empty()){
        cout<<s.top();
        s.pop();
    }
    cout<<endl;
    return 0;
}
