//insertion sort

#include <iostream>
#include<vector>
using namespace std;

int main()
{
    vector<int> v{10,1,7,6,14,9};
    int n=v.size();
    for(int i=1;i<n;i++){
        int val=v[i];
        int j;
        for(j=i-1;j>=0;j--){
            if(val<v[j]){
                v[j+1]=v[j];
            }
            else{
                break;
            }
        }
        v[j+1]=val;
    }
    //printing
    for(auto i: v){
        cout<<i<<" ";
    }
    return 0;
}
