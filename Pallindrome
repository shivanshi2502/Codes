#include <iostream>
#include<string>
#include<cstring>
using namespace std;

int main()
{
    char st[1000];
    cin.getline(st,1000);
    int len= strlen(st);
    int i=0;
    int j=len-1;
    while(i<=j){
        if(st[i]==st[j]){
            i++;
            j--;
        }
        else{
            cout<<"not pallindrome"<<endl;
            return 0;
        }
    }
    cout<<"pallindrome"<<endl;
}
