//Missing element in sorted array from 1-n

#include <iostream>
#include<algorithm>
using namespace std;
int binarySearch(int arr[], int size){
    int ans=-1;
    int s=0;
    int e=size-1;
    int m=((e-s)/2)+s;
    while(s<=e){
        if(arr[m]==m+1){
            s=m+1;
        }
        else{
            ans=m+1;
            e=m-1;
        }
        m=((e-s)/2)+s;
    }
    return ans;
}
int main ()
{
  int n;
  cout << "enter size of array" << endl;
  cin >> n;
  int arr[1000];
  cout << "enter elements in array" << endl;
  for (int i = 0; i < n; i++)
    {
      cin >> arr[i];
    }
  int ans=binarySearch(arr,n);
  cout<<"missing element is "<<ans<<endl;
  return 0;
}
