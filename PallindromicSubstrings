int checkPalindrome(string s, int start, int end, int count){
       // int count=0;
        while(start>=0 && end<s.size() && s[start]==s[end]){
            count++;
            start--;
            end++;
        }
        return count;
    }
    int countSubstrings(string s) {
        int OddCount=0;
        int EvenCount=0;
        for(int i=0;i<s.size();i++){
            OddCount=checkPalindrome(s,i,i,OddCount);
            EvenCount=checkPalindrome(s,i,i+1,EvenCount);
        }
        return OddCount+EvenCount;
    }
