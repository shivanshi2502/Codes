int strStr(string haystack, string needle) {
if(haystack==needle) return 0;
        if(haystack.length()<needle.length()){
            return -1;
        }
        for(int i=0;i<=(haystack.length()-needle.length());i++){
            string s=haystack.substr(i,needle.length());
            if(s==needle){
                return i;
            }
        }
        return -1;
    }
